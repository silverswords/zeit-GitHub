{
    "version": 2,
    "name": "GitHub",
    "builds": [
        { "src": "/*.go", "use": "@now/go" }
        
    ],
    "routes": [
        { "src": "/api/v1/github/user", "dest": "user.go" },
        { "src": "/api/v1/github/user/gist", "dest": "gist_list.go" },
        { "src": "/api/v1/github/repo/list", "dest": "list.go" },
        { "src": "/api/v1/github/repo/content", "dest": "content_list.go" },
        { "src": "/api/v1/github/repo/commits", "dest": "commits_list.go" }
      
    ]
}
